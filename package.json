  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "peerDependencies": {
    "@tanstack/react-query": ">=5.0.0",
    "react": ">=18",
    "typescript": ">=5.0.4"
  },
